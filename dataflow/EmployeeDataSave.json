{
	"name": "EmployeeDataSave",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "EmployeeData",
						"type": "DatasetReference"
					},
					"name": "TransferEmployData"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "EmployData"
				}
			],
			"scriptLines": [
				"source(output(",
				"          empid as integer,",
				"          EmployeeName as string,",
				"          Dep as string,",
				"          Salary as decimal(10,0)",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> TransferEmployData",
				"TransferEmployData alterRow(upsertIf(1==1)) ~> EmployData"
			]
		}
	}
}